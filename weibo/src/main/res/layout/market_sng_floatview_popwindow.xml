            <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:id="@id/market_sng_floatview_popwindow_item_review" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/market_sng_floatview_popwindow_item_space">
