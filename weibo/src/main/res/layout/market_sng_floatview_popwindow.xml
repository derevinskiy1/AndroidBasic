        <ImageView android:id="@id/market_sng_floatview_popwindow_arrow_bottom" android:background="@drawable/market_sng_floatview_popwindow_arrow_bottom" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@id/market_sng_floatview_popwindow_container" android:layout_centerHorizontal="true" />
