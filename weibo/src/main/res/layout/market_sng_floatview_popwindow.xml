                <TextView android:textColor="#ffffea00" android:id="@id/market_sng_floatview_popwindow_item_review_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/market_sng_floatview_popwindow_item_text_margin" android:text="@string/market_sng_floatview_popwindow_review" />
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal" android:orientation="vertical" android:id="@id/market_sng_floatview_popwindow_item_danmaku" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/market_sng_floatview_popwindow_item_space">
